{
  "compilerOptions": {
    "target": "ES5",
    "moduleResolution": "node",
    "module": "commonjs",
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017"
    ],
    "noImplicitAny": false,
    "sourceMap": true,
    "outDir": "../../dist",
    "baseUrl": "./",
    "jsx": "preserve",
    "allowJs": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "noEmit": false,
    "paths": {
      "*": ["node_modules/*"]
    },
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "./*"
  ],
  "exclude": [
    "../../node_modules/electron/node_modules/@types/node/*"
  ],
  "extends": "electron-webpack/tsconfig-base.json"
}
